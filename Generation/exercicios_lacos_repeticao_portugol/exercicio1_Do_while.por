programa
{
	
	funcao inicio()
	{
		inteiro i = 233

		escreva("\n", i)
		
		faca
		{
			se(i >= 300 e i <= 400)
			{
				i = i + 3

			}
			senao
			{
				i = i + 5
			}
			
			escreva("\n", i)
		}
		enquanto(i < 456)
	}
}
